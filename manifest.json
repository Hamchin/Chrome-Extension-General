{
  "name": "My General Extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "history",
    "downloads",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "background/script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "scripts/twitter/script.js"
      ]
    },
    {
      "matches": [
        "https://github.com/*/edit/*.md"
      ],
      "js": [
        "vendor/jquery.min.js",
        "scripts/github/script.js"
      ],
      "css": [
        "scripts/github/style.css"
      ]
    },
    {
      "matches": [
        "https://www.google.com/*",
        "https://www.google.co.jp/*"
      ],
      "css": [
        "scripts/google/style.css"
      ]
    },
    {
      "matches": [
        "https://drive.google.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "scripts/drive/script.js"
      ]
    },
    {
      "matches": [
        "https://colab.research.google.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "scripts/colab/script.js"
      ],
      "css": [
        "scripts/colab/style.css"
      ]
    },
    {
      "matches": [
        "https://www.mildom.com/*"
      ],
      "css": [
        "scripts/mildom/style.css"
      ]
    },
    {
      "matches": [
        "http://jin115.com/*"
      ],
      "css": [
        "scripts/blog/style.css"
      ]
    }
  ]
}
